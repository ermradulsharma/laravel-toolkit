{
    "name": "ermradulsharma/laravel-toolkit",
    "description": "A comprehensive toolkit for Laravel package and application development.",
    "keywords": [
        "laravel",
        "toolkit",
        "package-development",
        "helpers",
        "mradul"
    ],
    "homepage": "https://github.com/ermradulsharma/laravel-toolkit",
    "authors": [
        {
            "name": "Mradul Sharma",
            "email": "skywalkerlknw@gmail.com",
            "homepage": "https://github.com/ermradulsharma"
        }
    ],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "illuminate/console": "^12.0",
        "illuminate/contracts": "^12.0",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "laravel/framework": "^12.0",
        "orchestra/testbench-core": "^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Skywalker\\Support\\": "src/"
        },
        "files": [
            "helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Skywalker\\Support\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --colors=always",
        "test:dox": "phpunit --testdox --colors=always",
        "test:cov": "phpunit --coverage-html coverage"
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "12.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}